    304     V0TAX_CSI # Total indirect tax revenue #;
    305   Formula
    306     V1CAP_I   = sum{i,IND, V1CAP(i)};
    307     V1LAB_IO  = sum{i,IND, V1LAB_O(i)};
    308     V1LND_I   = sum{i,IND, V1LND(i)};
    309     V1PTX_I   = sum{i,IND, V1PTX(i)};
    310     V1OCT_I   = sum{i,IND, V1OCT(i)};
    311     V1PRIM_I  = V1LAB_IO + V1CAP_I + V1LND_I;
    312     V0TAX_CSI  = V1TAX_CSI + V2TAX_CSI + V3TAX_CS + V4TAX_C + V5TAX_CS
    313                  + V0TAR_C + V1OCT_I + V1PTX_I;
    314     V0GDPINC  = V1PRIM_I + V0TAX_CSI;
    315   Write V0GDPINC to file DIAG header "GINC";
    316   Coefficient
    317    (all,c,COM) V0CIF(c) # Total ex-duty imports of good c #;
    318    V0CIF_C  # Total local currency import costs, excluding tariffs #;
    319    V0IMP_C  # Total basic-value imports (includes tariffs) #;
    320    V2TOT_I  # Total investment usage #;
    321    V4TOT    # Total export earnings #;
    322    V5TOT    # Total value of government demands #;
    323    V6TOT    # Total value of inventories #;
    324    V0GNE    # GNE from expenditure side #;
    325    V0GDPEXP # GDP from expenditure side #;
    326   Formula
    327    (all,c,COM) V0CIF(c) = V0IMP(c) - V0TAR(c);
    328    V0CIF_C   = sum{c,COM, V0CIF(c)};
    329    V0IMP_C   = sum{c,COM, V0IMP(c)};
    330    V2TOT_I   = sum{i,IND, V2TOT(i)};
    331    V4TOT     = sum{c,COM, V4PUR(c)};
    332    V5TOT     = sum{c,COM, sum{s,SRC, V5PUR(c,s)}};
    333    V6TOT     = sum{c,COM, sum{s,SRC, V6BAS(c,s)}};
    334    V0GNE     = V3TOT + V2TOT_I + V5TOT + V6TOT;
    335    V0GDPEXP  = V0GNE + V4TOT - V0CIF_C;
    336   Write V0GDPEXP to file DIAG header "GEXP";
    337   Write V0IMP_C to file DIAG header "VIMP";
    338   Set DESTPLUS # Sale Categories #
    339    (Interm, INVEST, HouseH, Export, GovGE, STOCKS, Margins, Total);
    340   Subset DEST is subset of DESTPLUS;
    341   Set EXPMAC # Expenditure Aggregates #
    342    (Consumption, Investment, Government, STOCKS, Exports, Imports);
    343   Coefficient
    344    (all,i,IND) DIFFIND(i)  # COSTS-MAKE_C : should be zero #;
    345    (all,c,COM) DIFFCOM(c)  # SALES-MAKE_I : should be zero #;
    346   Formula
    347    (all,i,IND) DIFFIND(i)  = V1TOT(i) - MAKE_C(i);
    348    (all,c,COM) DIFFCOM(c)  = SALES(c) - MAKE_I(c);
    349   Write
    350    DIFFIND to file DIAG header "DIND";
    351    DIFFCOM to file DIAG header "DCOM";
    352    EPSTOT  to file DIAG header "ETOT";
    353    MAKE    to file DIAG header "MAKE";
    354    V1TOT   to file DIAG header "1TOT";
    355    SALES   to file DIAG header "SLES";
    356    
    357   Assertion
    358    # V1TOT>0 # (all,i,IND) V1TOT(i)>0;
    359    # SALES>0 # (all,c,COM) SALES(c)>0;
    360    # (STOCKS/SALES) <95% # (all,c,COM) [V6BAS(c,"dom")/SALES(c)]<0.95;
    361    
    362   Formula
    363    (all,i,IND) DIFFIND(i)  = DIFFIND(i)/V1TOT(i);
    364    (all,c,COM) DIFFCOM(c)  = DIFFCOM(c)/SALES(c);
    365   Write
    366    DIFFIND to file DIAG header "SIND" longname "DIFFIND(i)/V1TOT(i)";
    367    DIFFCOM to file DIAG header "SCOM" longname "DIFFCOM(c)/SALES(c)";
    368   Assertion
    369    # DIFFIND = V1TOT-MAKE_C = tiny # (all,i,IND) ABS[DIFFIND(i)] <0.001;
    370    # DIFFCOM = SALES-MAKE_I = tiny # (all,c,COM) ABS[DIFFCOM(c)] <0.001;
    371    (initial) # Average Engel elasticity = 1 #  ABS[1-EPSTOT] <0.001;
    372   Coefficient (all,e,EXPMAC)  EXPGDP(e) # Expenditure Aggregates #;
    373   Formula
    374    EXPGDP("Consumption") = V3TOT;
    375    EXPGDP("Investment")  = V2TOT_I;
    376    EXPGDP("Government")  = V5TOT;
    377    EXPGDP("Stocks")      = V6TOT;
    378    EXPGDP("Exports")     = V4TOT;
    379    EXPGDP("Imports")     = -V0CIF_C;
    380   Write EXPGDP to file DIAG header "EMAC";
    381   Set INCMAC # Income Aggregates #
    382    (Land, Labour, Capital,  IndirectTax);
    383   Coefficient (all,i,INCMAC)  INCGDP(i) # Income Aggregates #;
    384   Formula
    385    INCGDP("Land")        = V1LND_I;
    386    INCGDP("Labour")      = V1LAB_IO;
    387    INCGDP("Capital")     = V1CAP_I;
    388    INCGDP("IndirectTax") = V0TAX_CSI;
    389   Write INCGDP to file DIAG header "IMAC";
    390   Set TAXMAC # Tax Aggregates #
    391     (Intermediate,Investment,Consumption,Exports,Government,OCT,ProdTax,Tariff);
    392   Coefficient (all,t,TAXMAC)  TAX(t) # Tax Aggregates #;
    393   Formula
    394    TAX("Intermediate") = V1TAX_CSI;
    395    TAX("Investment")   = V2TAX_CSI;
    396    TAX("Consumption")  = V3TAX_CS;
    397    TAX("Exports")      = V4TAX_C;
    398    TAX("Government")   = V5TAX_CS;
    399    TAX("OCT")          = V1OCT_I;
    400    TAX("ProdTax")      = V1PTX_I;
    401    TAX("Tariff")       = V0TAR_C;
    402   Write TAX to file DIAG header "TMAC";
    403   Set COSTCAT # Cost Categories #
    404    (IntDom, IntImp, Margin, ComTax, Lab, Cap, Lnd, ProdTax, OCT);
    405   Coefficient (all,i,IND)(all,co,COSTCAT) COSTMAT(i,co) # Cost Matrix #;
    406   Formula
    407    (all,i,IND) COSTMAT(i,"IntDom")  = sum{c,COM, V1BAS(c,"dom",i)};
    408    (all,i,IND) COSTMAT(i,"IntImp")  = sum{c,COM, V1BAS(c,"imp",i)};
    409    (all,i,IND) COSTMAT(i,"Margin")  =
    410                         sum{c,COM, sum{s,SRC, sum{m,MAR, V1MAR(c,s,i,m)}}};
    411    (all,i,IND) COSTMAT(i,"ComTax")  = sum{c,COM, sum{s,SRC, V1TAX(c,s,i)}};
    412    (all,i,IND) COSTMAT(i,"Lab")     = V1LAB_O(i);
    413    (all,i,IND) COSTMAT(i,"Cap")     = V1CAP(i);
    414    (all,i,IND) COSTMAT(i,"Lnd")     = V1LND(i);
    415    (all,i,IND) COSTMAT(i,"ProdTax") = V1PTX(i);
    416    (all,i,IND) COSTMAT(i,"OCT")     = V1OCT(i);
    417   Write COSTMAT to file DIAG header "CSTM";
    418   Set
    419   SALECAT2 # SALE Categories # (Interm, INVEST, HouseH, Export, GovGE, STOCKS);
    420   FLOWTYPE # Type of flow # (Basic, Margin, TAX);
    421   Coefficient
    422   (all,c,COM)(all,f,FLOWTYPE)(all,s,SRC)(all,sa,SALECAT2) SALEMAT2(c,f,s,sa)
    423       # Basic, margin and tax components of purchasers' values #;
    424   Formula
    425   (all,c,COM)(all,f,FLOWTYPE)(all,s,SRC)(all,sa,SALECAT2) SALEMAT2(c,f,s,sa)=0;
    426   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Basic",s,"Interm") = sum{i,IND,V1BAS(c,s,i)};
    427   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Tax"  ,s,"Interm") = sum{i,IND,V1TAX(c,s,i)};
    428   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Margin",s,"Interm") =
    429                                         sum{i,IND, sum{m,MAR, V1MAR(c,s,i,m)}};
    430   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Basic",s,"Invest") = sum{i,IND,V2BAS(c,s,i)};
    431   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Tax"  ,s,"Invest") = sum{i,IND,V2TAX(c,s,i)};
    432   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Margin",s,"Invest") =
    433                                         sum{i,IND, sum{m,MAR, V2MAR(c,s,i,m)}};
    434   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Basic",s,"HouseH") = V3BAS(c,s);
    435   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Tax"  ,s,"HouseH") = V3TAX(c,s);
    436   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Margin",s,"HouseH")= sum{m,MAR,V3MAR(c,s,m)};
    437   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Basic",s,"GovGE") = V5BAS(c,s);
    438   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Tax"  ,s,"GovGE") = V5TAX(c,s);
    439   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Margin",s,"GovGE")= sum{m,MAR,V5MAR(c,s,m)};
    440   (all,c,COM)            SALEMAT2(c,"Basic","dom","Export") = V4BAS(c);
    441   (all,c,COM)            SALEMAT2(c,"Tax"  ,"dom","Export") = V4TAX(c);
    442   (all,c,COM)          SALEMAT2(c,"Margin","dom","Export")= sum{m,MAR,V4MAR(c,m)};
    443   (all,c,COM)(all,s,SRC) SALEMAT2(c,"Basic",s,"Stocks") = V6BAS(c,s);
    444   Write SALEMAT2 to file DIAG header "MKUP";
    445   Coefficient  (all,c,COM) IMPSHR(c) # Share of imports in local market #;
    446   Formula      (all,c,COM) IMPSHR(c) = V0IMP(c)/[DOMSALES(c)+V0IMP(c)];
    447   Write IMPSHR to file DIAG header "MSHR";
    448   Coefficient (all,i,IND) SUPPLYELAST(i) # Short-run supply elasticity #;
    449   Zerodivide default 999;
    450   Zerodivide (nonzero_by_zero) default 999;
    451   Formula     (all,i,IND) SUPPLYELAST(i) =
    452       SIGMA1PRIM(i)*V1LAB_O(i)*V1TOT(i)/[V1PRIM(i)*{V1CAP(i)+V1LND(i)}];
    453   Zerodivide off;
    454   Zerodivide (nonzero_by_zero) off;
    455   Write SUPPLYELAST to file DIAG header "SRSE";
    456   Set FAC # Primary Factors #  (Lab, Cap, Lnd);
    457   Coefficient (all,i,IND)(all,f,FAC) FACTOR(i,f) # Primary Factor Costs #;
    458   Formula
    459    (all,i,IND) FACTOR(i,"Lab")     = V1LAB_O(i);
    460    (all,i,IND) FACTOR(i,"Cap")     = V1CAP(i);
    461    (all,i,IND) FACTOR(i,"Lnd")     = V1LND(i);
    462   Write FACTOR to file DIAG header "FACT";
    463   Coefficient (all,c,COM) V1PUR_SI(c)  # Dom+imp intermediate purch. value #;
    464               (all,c,COM) V2PUR_SI(c)  # Dom+imp investment purch. value #;
    465               (all,c,COM) V5PUR_S(c)   # Dom+imp government purch. value #;
    466               (all,c,COM) V6BAS_S(c)   # Dom+imp inventories #;
    467   Formula     (all,c,COM) V1PUR_SI(c)  = sum{i,IND, V1PUR_S(c,i)};
    468               (all,c,COM) V2PUR_SI(c)  = sum{i,IND, V2PUR_S(c,i)};
    469               (all,c,COM) V5PUR_S(c)   = sum{s,SRC, V5PUR(c,s)};
    470               (all,c,COM) V6BAS_S(c)   = sum{s,SRC, V6BAS(c,s)};
    471   Write
    472    V4PUR    to file DIAG header "4PUR";
    473    V1LAB_O  to file DIAG header "LAB1";
    474    V1PRIM   to file DIAG header "VLAD";
    475    V1PUR_SI to file DIAG header "1PUR";
    476    V2PUR_SI to file DIAG header "2PUR";
    477    V3PUR_S  to file DIAG header "3PUR";
    478   Coefficient  (all,c,COM) TARFRATE(c) # Ad valorem tariff rate #;
    479   Formula      (all,c,COM) TARFRATE(c) = 0;
    480   Formula      (all,c,COM:V0CIF(c)>0) TARFRATE(c) = V0TAR(c)/V0CIF(c);
    481   Write
    482     TARFRATE to file DIAG header "TRAT";
    483     V0TAR    to file DIAG header "0TAR";
    484     V0CIF    to file DIAG header "0CIF";
    485     V0IMP    to file DIAG header "0IMP";
    486     V1CAP    to file DIAG header "1CAP";
    487     V6BAS_S  to file DIAG header "6BSS";
    488     V2TOT    to file DIAG header "2TOT";
    489     V5PUR_S  to file DIAG header "5PUR";
    490   Assertion # V0CIF>0 if V0TAR<>0 # (all,c,COM:V0TAR(c)<>0) V0CIF(c)>0;
    491    
    492   Set LOCCOM      # Local Commodities (Set L, DPSV p.259) #
    493      = (all,c,COM: ISLOCCOM(c) > 0.5 );
    494   Set  NATCOM = COM - LOCCOM;
    495   Set LOCIND # Local Industries= 90% of output is local commodities #
    496     = (all,i,IND: sum{c,LOCCOM,MAKE(c,i)}/sum{c,COM,MAKE(c,i)}>0.9);
    497   Write
    498    (Set) LOCIND to file DIAG header "LOCI";
    499    (Set) LOCCOM to file DIAG header "LOCC";
    500   Coefficient
    501   (all,i,IND)(all,r,REG) REGV1LAB_O(i,r) # Labour bills #;
    502   (all,r,REG)            REGV1LAB_IO(r)  # Labour bills #;
    503   (all,r,REG)            REGINCSHR(r)  # Share region r in national labour bill #;
    504   Formula
    505   (all,i,IND)(all,r,REG) REGV1LAB_O(i,r) = V1LAB_O(i)*R007(i,r); !JLe ************************************!
    506   (all,r,REG)            REGV1LAB_IO(r)  = sum{i,IND, REGV1LAB_O(i,r)};
    507   (all,r,REG)            REGINCSHR(r)  = REGV1LAB_IO(r)/ V1LAB_IO;
    508   Write
    509    REGV1LAB_IO to file DIAG header "RV1L";
    510    REGINCSHR to file DIAG header "RISR";
    511    R001 to file DIAG header "R001";
    512    R002 to file DIAG header "R002";
    513    R003 to file DIAG header "R003";
    514    R004 to file DIAG header "R004";
    515    R005 to file DIAG header "R005";
    516    R006 to file DIAG header "R006";
    517    R007 to file DIAG header "R007"; !JLe ************************************!
    518   Assertion
    519   # Sum-over-REG R001 = 1 # (all,i,IND) ABS[1-sum{r,REG,R001(i,r)}] <0.001;
    520   # Sum-over-REG R002 = 1 # (all,i,IND) ABS[1-sum{r,REG,R002(i,r)}] <0.001;
    521   # Sum-over-REG R003 = 1 # (all,c,COM) ABS[1-sum{r,REG,R003(c,r)}] <0.001;
    522   # Sum-over-REG R004 = 1 # (all,c,COM) ABS[1-sum{r,REG,R004(c,r)}] <0.001;
    523   # Sum-over-REG R005 = 1 # (all,c,COM) ABS[1-sum{r,REG,R005(c,r)}] <0.001;
    524   # Sum-over-REG R006 = 1 # (all,c,COM) ABS[1-sum{r,REG,R006(c,r)}] <0.001;
    525   # Sum-over-REG R007 = 1 # (all,i,IND) ABS[1-sum{r,REG,R007(i,r)}] <0.001; !JLe ************************************!
    526   Coefficient  (all,c,COM)(all,r,REG)
    527    TOTSUPREG(c,r) # Supply of good c region r #;
    528   Formula      (all,c,COM)(all,r,REG)
    529    TOTSUPREG(c,r) = sum{i,IND, MAKE(c,i)*R001(i,r)};
    530   Coefficient  (all,c,COM)(all,r,REG)
    531    TOTDEMREG(c,r) # All basic + margin use of local good c, region r #;
    532   Formula
    533    (all,c,COM)(all,r,REG) TOTDEMREG(c,r) =
    534      sum{i,IND,R001(i,r)*V1BAS(c,"dom",i) + R002(i,r)*V2BAS(c,"dom",i) }
    535    +           R003(c,r)*V3BAS(c,"dom")
    536    +           R004(c,r)*V4BAS(c)
    537    +           R005(c,r)*V5BAS(c,"dom")
    538    +           R006(c,r)*V6BAS(c,"dom");
    539   Formula
    540    (all,c,MAR)(all,r,REG) TOTDEMREG(c,r) = TOTDEMREG(c,r)
    541     + sum{u,COM,R004(u,r)*V4MAR(u,c)
    542     + sum{s,SRC,R003(u,r)*V3MAR(u,s,c)   + R005(u,r)*V5MAR(u,s,c)
    543     + sum{i,IND,R001(i,r)*V1MAR(u,s,i,c) + R002(i,r)*V2MAR(u,s,i,c)}}};
    544    
    545   Coefficient (all,c,COM)(all,r,REG) DEMRATIO(c,r)
    546    # Regional demand/supply; -ve for national coms, should be near 1 for local #;
    547   Formula     (all,c,COM)(all,r,REG)
    548   DEMRATIO(c,r) = [TINY + TOTDEMREG(c,r)]/[TINY + TOTSUPREG(c,r)];
    549   Formula  (all,c,NATCOM)(all,r,REG) DEMRATIO(c,r) = - DEMRATIO(c,r);
    550   Write
    551   TOTSUPREG to file DIAG header "TSPR";
    552   TOTDEMREG to file DIAG header "TDMR";
    553   DEMRATIO  to file DIAG header  "DMRT";
    554   Coefficient
    555   (all,i,IND)(all,r,REG) REGV1PRIM(i,r)  # Factor bills #;
    556   (all,i,IND)(all,r,REG) R00PRIM(i,r)    # Share ind i in output region r #;
    557   (all,r,REG)            REGV1PRIM_I(r)  # Total factor bill #;
    558   Formula
    559   (all,i,IND)(all,r,REG) REGV1PRIM(i,r)  = V1PRIM(i)*R001(i,r);
    560   (all,r,REG)            REGV1PRIM_I(r) = sum{i,IND,REGV1PRIM(i,r)};
    561   (all,i,IND)(all,r,REG) R00PRIM(i,r) = REGV1PRIM(i,r)/REGV1PRIM_I(r);
    562   Write R00PRIM to file DIAG header "RSRP";
    563       REGV1PRIM to file DIAG header "RV1P";
    564    
    565   Coefficient
    566   (all,c,COM)(all,r,REG) TOTIMPREG(c,r) # All use of imported good c, region r #;
    567   (all,c,COM)            TOTIMP(c)      # All use of imported good c #;
    568   (all,r,REG)            REGIMPSHR(r)   # Share of Import use #;
    569                          IMPTOT         # Temp total #;
    570   Formula
    571    (all,c,COM)(all,r,REG) TOTIMPREG(c,r) =
    572      sum{i,IND,R001(i,r)*V1BAS(c,"imp",i) + R002(i,r)*V2BAS(c,"imp",i)}
    573              + R003(c,r)*V3BAS(c,"imp")
    574              + R005(c,r)*V5BAS(c,"imp")
    575              + R006(c,r)*V6BAS(c,"imp");
    576    (all,c,COM) TOTIMP(c) = sum{r,ORG, TOTIMPREG(c,r)};
    577    (all,r,REG) REGIMPSHR(r) = sum{c,COM, TOTIMPREG(c,r)}; ! a total !
    578    IMPTOT = sum{r,REG, REGIMPSHR(r)};
    579    (all,r,REG) REGIMPSHR(r) =  REGIMPSHR(r)/IMPTOT; ! convert to share !
    580   Write TOTIMPREG to file DIAG header "TIMR";
    581    
    582   ! here we use regional use share of imports as a guesstimate for
    583     share of national imports, by port of entry !
    584   Coefficient (all,c,COM)(all,r,ORG) MSHR(c,r)
    585    # Share of national imports, by port of entry #;
    586   Formula (all,c,COM)(all,r,ORG) MSHR(c,r) = REGIMPSHR(r);
    587           (all,c,COM:TOTIMP(c)>0)(all,r,ORG) MSHR(c,r) = TOTIMPREG(c,r)/TOTIMP(c);
    588   Write MSHR to file DUMSUPP header "MSHR";
