     55   (all,c,COM)(all,s,SRC)(all,m,MAR) V3MAR(c,s,m) # Households Margins #;
     56   (all,c,COM)(all,m,MAR) V4MAR(c,m) # Exports Margins #;
     57   (all,c,COM)(all,s,SRC)(all,m,MAR) V5MAR(c,s,m) # Government Margins #;
