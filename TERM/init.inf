    182   !************* Check database adds up ****************!
    183    
    184   Coefficient
    185    (all,c,COM)(all,s,SRC)(all,i,IND)  V1PUR(c,s,i)  # Intermediate purch. value #;
    186    (all,c,COM)(all,s,SRC)(all,i,IND)  V2PUR(c,s,i)  # Investment purch. value #;
    187    (all,c,COM)(all,s,SRC)             V3PUR(c,s)    # Households purch. value #;
    188    (all,c,COM)                        V4PUR(c)      # Export purch. value #;
    189    (all,c,COM)(all,s,SRC)             V5PUR(c,s)    # Government purch. value #;
    190   Formula
    191    (all,c,COM)(all,s,SRC)(all,i,IND)
    192      V1PUR(c,s,i)  = V1BAS(c,s,i) + V1TAX(c,s,i) + sum{m,MAR, V1MAR(c,s,i,m)};
    193    (all,c,COM)(all,s,SRC)(all,i,IND)
    194      V2PUR(c,s,i)  = V2BAS(c,s,i) + V2TAX(c,s,i) + sum{m,MAR, V2MAR(c,s,i,m)};
    195    (all,c,COM)(all,s,SRC)
    196      V3PUR(c,s)    = V3BAS(c,s)   + V3TAX(c,s)   + sum{m,MAR, V3MAR(c,s,m)};
    197    (all,c,COM)
    198      V4PUR(c)      = V4BAS(c)     + V4TAX(c)     + sum{m,MAR, V4MAR(c,m)};
    199    (all,c,COM)(all,s,SRC)
    200      V5PUR(c,s)    = V5BAS(c,s)   + V5TAX(c,s)   + sum{m,MAR, V5MAR(c,s,m)};
