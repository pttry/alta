    212    (all,c,COM)(all,i,IND)            V1PUR_S(c,i) = sum{s,SRC, V1PUR(c,s,i)};
    213    (all,i,IND)                       V1MAT(i)     = sum{c,COM, V1PUR_S(c,i)};
