    626    EXP_ELAST_NT to file OUTFILE header "EXNT" longname
    627    "Collective Export Elasticity";
    628    ISINDIVEXP to file OUTFILE header "ITEX" longname
    629    "Flag, >0.5 for individual export commodities, else collective export";
    630    ISLOCCOM to file OUTFILE header "LCOM" longname
    631    "Flag for regional extension, >0.5 for local coms, else national";
    632    R001 to file OUTFILE header "R001" longname "Regional output shares";
    633    R002 to file OUTFILE header "R002" longname "Regional investment shares";
    634    R003 to file OUTFILE header "R003" longname "Regional consumption shares";
    635    R004 to file OUTFILE header "R004" longname "Regional export shares";
    636    R005 to file OUTFILE header "R005" longname "Regional government shares";
    637   ! R006 to file OUTFILE header "R006" longname "Regional inventory shares";!
    638    R007 to file OUTFILE header "R007" longname "Regional labour compensation shares"; !JLe ************************************!
    639    
    640   !********************************************************************!
    641   ! Create dummy file of supplementary regional data for later editing !
    642    
    643   ! Make preliminary estimate of regional shares of GDP !
    644   Coefficient
    645   (all,r,REG) DEFREGSHR(r) # Default regional share #;
    646               TEMTOT       # Temporary total #;
    647   Formula
    648   (all,r,REG) DEFREGSHR(r) = sum{i,IND,R001(i,r)*V1PRIM(i)};
    649               TEMTOT = sum{r,REG,DEFREGSHR(r)};
    650   (all,r,REG) DEFREGSHR(r) = DEFREGSHR(r)/TEMTOT;
    651    
    652   Coefficient (all,r,REG)(all,d,REG) DISTANCE(r,d)
    653    # Shortest distance from r to d #;
    654   Formula (all,r,REG)(all,d,REG)
    655     DISTANCE(r,d) = 2 + ABS[$pos(r)-$pos(d)];
    656   Write DISTANCE to file DUMSUPP header "DIST";
    657    
    658   Coefficient (all,c,COM)(all,s,SRC) DISTFAC(c,s)
    659    # Distance factor for gravity formula #;
    660   Formula (all,c,COM)(all,s,SRC)
    661     DISTFAC(c,s) =  1.5*{(NCOM/2) + ABS[$pos(c) - (NCOM/2)]}/NCOM;
    662   Write DISTFAC to file DUMSUPP header "DFAC";
    663    
    664   Coefficient (all,r,REG)(all,m,MAR) MARWGT(r,m) # Margin weighting #;
    665   Formula (all,r,REG)(all,m,MAR) MARWGT(r,m) = 1.0;
    666   Write MARWGT to file DUMSUPP header "MWGT";
    667    
    668   Set DMAR # Distance related margins #
    669    = (all,m,MAR: $pos(m)>0.5*NMAR); ! ie, second half of MAR !
    670   Write (Set) DMAR to file DUMSUPP header "DMAR";
    671    
    672   ! We would expect that some margins (eg, retail) would be mainly sourced
    673     in the region where good is used -- For these goods set LOCMAR to, say, 5.
    674     Otherwise, set LOCMAR=1 !
    675   Coefficient (all,m,MAR) LOCMAR(m) # Tendency for margin to be locally sourced #;
    676   Formula     (all,m,MAR) LOCMAR(m) = 1;
    677   Write LOCMAR to file DUMSUPP header "LMAR";
    678    
    679   Coefficient (all,r,REG) RPOP(r) # Regional population #;
    680   Formula (all,r,REG) RPOP(r) = DEFREGSHR(r)*10000000;
    681   Write RPOP to file DUMSUPP header "PO01";
    682    
    683   Coefficient (all,c,COM) SIGMADOMDOM(c) # Between-region Armington #;
    684   Formula (all,c,COM) SIGMADOMDOM(c) = 5.0;
    685   Write SIGMADOMDOM to file DUMSUPP header "SGDD" longname
    686    "Between-region Armington";
    687    
    688   Coefficient (all,m,MAR) SIGMAMAR(m)
    689    # Elasticity of substitution between regions of margin production #;
    690   Formula (all,m,MAR) SIGMAMAR(m) = 0.2;
    691   Write SIGMAMAR to file DUMSUPP header "SMAR" longname
    692    "Elasticity of substitution between regions of margin production";
    693    
    694   Coefficient (all,c,COM) XX(c);
    695   Formula (all,c,COM) XX(c) = sum{r,REG, ABS[1-DEMRATIO(c,r)]}/NREG;
    696   Set SELFSUFFIC = (all,c,COM: XX(c)<0.01);
    697   Set RLOC # Truly local commodities # = LOCCOM intersect SELFSUFFIC;
    698   Write (Set) RLOC to file DUMSUPP header "RLOC";
    699    
    700   ! Create dummy aggregation files !
    701    
    702   File (new) SECAGG # Dummy sector aggregation instructions #;
    703   File (new) REGAGG # Dummy region aggregation instructions #;
    704    
    705   ! identity mapping !
    706   Set AGGREG # Aggregated Regions # = REG;
    707   Write (Set) AGGREG to file REGAGG header "AREG";
    708   Mapping (onto) MAPREG from REG to AGGREG;
    709   Formula (all,r,REG) MAPREG(r)= $pos(r);
    710   Write (by_elements) MAPREG to file REGAGG header "MREG";
    711    
    712   Set AGGCOM # Aggregated commodities # (COM01 - COM10);
    713   Write (Set) AGGCOM to file SECAGG header "ACOM";
    714   Coefficient (all,c,COM) MPC(c);
    715   Formula (all,c,COM) MPC(c) = 1+TRUNC0[10*{$pos(c)-1}/NCOM];
    716   Mapping (onto) MAPCOM from COM to AGGCOM;
    717   Formula (all,c,COM) MAPCOM(c)= TRUNC0(MPC(c));
    718   Write (by_elements) MAPCOM to file SECAGG header "MCOM";
    719    
    720   Set AGGIND # Aggregated Industries # (IND01 - IND10);
    721   Write (Set) AGGIND to file SECAGG header "AIND";
    722   Coefficient (all,i,IND) MPI(i);
    723   Formula (all,i,IND) MPI(i) = 1+TRUNC0[10*{$pos(i)-1}/NIND];
    724   Mapping (onto) MAPIND from IND to AGGIND;
    725   Formula (all,i,IND) MAPIND(i)= TRUNC0(MPI(i));
    726   Write (by_elements) MAPIND to file SECAGG header "MIND";
    727    
    728   !
    729   Coefficient NIND2r # New number of industries #;
    730   Formula     NIND2r = 0.1+ NIND/2;
    731   Coefficient (integer) NIND2 # New number of industries #;
    732   Formula     NIND2  = ROUND[NIND2r];
    733   Set (intertemporal) AGGIND # Aggregated industries # (IND[1] - IND[NIND2]);
    734   Write (set) AGGIND to file SECAGG header "AIND";
    735   Coefficient (all,i,IND) MPI(i);
    736   Formula (all,i,IND) MPI(i) = ROUND[0.1+ $POS(i)/2];
    737   Mapping (onto) MAPIND from IND to AGGIND;
    738   Formula (all,i,IND) MAPIND(i)= TRUNC0(MPI(i));
    739   Write (by_elements) MAPIND to file SECAGG header "MIND";
    740   !
    741   !end file!




