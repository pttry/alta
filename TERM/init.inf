    282   Coefficient
    283    V1TAX_CSI  # Total intermediate tax revenue #;
    284    V2TAX_CSI  # Total investment tax revenue #;
    285    V3TAX_CS   # Total households tax revenue #;
    286    V4TAX_C    # Total export tax revenue #;
    287    V5TAX_CS   # Total government tax revenue #;
    288    V0TAR_C    # Total tariff revenue #;
    289   Formula
    290    V1TAX_CSI  = sum{c,COM, sum{s,SRC, sum{i,IND, V1TAX(c,s,i)}}};
    291    V2TAX_CSI  = sum{c,COM, sum{s,SRC, sum{i,IND, V2TAX(c,s,i)}}};
